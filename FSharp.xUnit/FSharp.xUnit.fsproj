<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<Authors>xp44m</Authors>
		<Company>34696643@qq.com</Company>
		<PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
		<Description>The class library extends `xunit` to assert equal. It output data in F# style. It's easier to customize the IEqualityComparer.</Description>
		<Copyright>xp44mm</Copyright>
		<RepositoryUrl>https://github.com/xp44mm/FSharp.xUnit</RepositoryUrl>
		<PackageProjectUrl>https://github.com/xp44mm/FSharp.xUnit</PackageProjectUrl>
		<Version>1.2.3</Version>
		<PackageReleaseNotes>ClassDataBase</PackageReleaseNotes>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="EqualityComparers\NullableEqualityComparer.fs" />
		<Compile Include="EqualityComparers\ArrayEqualityComparer.fs" />
		<Compile Include="EqualityComparers\TupleEqualityComparer.fs" />
		<Compile Include="EqualityComparers\RecordEqualityComparer.fs" />
		<Compile Include="EqualityComparers\ListEqualityComparer.fs" />
		<Compile Include="EqualityComparers\SetEqualityComparer.fs" />
		<Compile Include="EqualityComparers\MapEqualityComparer.fs" />
		<Compile Include="EqualityComparers\UnionEqualityComparer.fs" />
		<Compile Include="EqualityComparers\SeqEqualityComparer.fs" />

		<Compile Include="EqualityComparer.fs" />
		<Compile Include="EqualConfig.fs" />
		<Compile Include="Should.fs" />
		<Compile Include="ClassDataBase.fs" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="FSharp.Idioms" Version="1.2.8" />
		<PackageReference Include="FSharp.Literals" Version="2.2.9" />
		<PackageReference Include="xunit.assert" Version="2.4.1" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Update="FSharp.Core" Version="6.0.4" />
	</ItemGroup>

</Project>
